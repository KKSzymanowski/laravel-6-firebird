{
  "name": "kkszymanowski/laravel-6-firebird",
  "description": "A Firebird database package for Laravel 6",
  "license": "MIT",
  "authors": [
    {
      "name": "Kuba Szymanowski",
      "email": "kubaszymanowski@gmail.com"
    },
    {
      "name": "Jacques van Zuydam",
      "email": "jtvanzuydam@gmail.com"
    }
