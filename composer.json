  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2.0",
    "ext-pdo": "*",
    "illuminate/support": "^6.0|^7.0|^8.0",
    "illuminate/database": "^6.0|^7.0|^8.0"
  },
  "extra": {
    "laravel": {
